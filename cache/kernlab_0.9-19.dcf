Package: kernlab
Version: 0.9-19
Title: Kernel-based Machine Learning Lab
Authors@R: c(person("Alexandros", "Karatzoglou", role = c("aut", "cre"),
                    email = "alexis@ci.tuwien.ac.at"),
             person("Alex", "Smola", role = "aut"),
