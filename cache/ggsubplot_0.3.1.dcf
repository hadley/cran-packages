        'geom_subplot.r' 'geom_subplot2d.r' 'ggsubplot-class.r'
        'ggsubplot_build.r' 'merge_overlaps.r' 'package.r' 'ply_aes.r'
        'ref_box.r' 'rel-class.r' 'rescalers.r' 'sp_layer-class.r'
        'sp_layer_build.r' 'util.r' 'geom_freqstar.r'
