        'class-PiecewiseLinearFuzzyNumber.R' 'class-PowerFuzzyNumber.R'
        'methods-core.R' 'methods-supp.R' 'methods-value.R'
        'methods-expectedValue.R' 'methods-weightedExpectedValue.R'
        'methods-width.R' 'methods-ambiguity.R' 'methods-plot.R'
